{
  "name": "developing4fun/app-skeleton",
  "description": "Basic skeleton for incoming projects. This skeleton provides a basic folder structure and a simple Docker environment.",
  "type": "project",
  "authors": [
    {
      "name":"thefoovault"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shared\\": "src/Shared",
      "CauldronOverflow\\": "src/CauldronOverflow",
      "CauldronOverflowWeb\\": "apps/CauldronOverflowWeb/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CauldronOverflow\\Test\\": "test"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "johnkary/phpunit-speedtrap": "^3.2",
    "symfony/maker-bundle": "^1.28"
  },
  "require": {
    "symfony/framework-bundle": "^5.2",
    "symfony/dotenv": "^5.2",
    "symfony/yaml": "^5.2",
    "symfony/console": "^5.2",
    "symfony/flex": "^1.11",
    "symfony/twig-bundle": "^5.2",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "symfony/debug-bundle": "^5.2",
    "symfony/monolog-bundle": "^3.0",
    "symfony/stopwatch": "^5.2",
    "symfony/web-profiler-bundle": "^5.2",
    "symfony/var-dumper": "^5.2",
    "symfony/asset": "^5.2",
    "knplabs/knp-markdown-bundle": "^1.8",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.8",
    "symfony/proxy-manager-bridge": "^5.2",
    "ext-json": "^7.4",
    "ramsey/uuid": "^4.1",
    "knplabs/knp-time-bundle": "^1.15"
  },
  "extra": {
    "bin-dir": "apps/CauldronOverflowWeb/bin",
    "config-dir": "apps/CauldronOverflowWeb/config",
    "src-dir": "apps/CauldronOverflowWeb/src",
    "var-dir": "apps/CauldronOverflowWeb/var",
    "public-dir": "apps/CauldronOverflowWeb/public"
  },
  "scripts": {
    "auto-scripts": {

    }
  }
}
